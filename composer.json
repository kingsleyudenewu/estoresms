{
    "name": "kingsleyudenewu/estoresms",
    "description": "A laravel package for estore sms API",
    "keywords": [
        "kingsleyudenewu",
        "estoresms"
    ],
    "homepage": "https://github.com/kingsleyudenewu/estoresms",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Udenewu Kingsley",
            "email": "kingzpacking@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Kingsleyudenewu\\Estoresms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kingsleyudenewu\\Estoresms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kingsleyudenewu\\Estoresms\\EstoresmsServiceProvider"
            ],
            "aliases": {
                "Estoresms": "Kingsleyudenewu\\Estoresms\\EstoresmsFacade"
            }
        }
    }
}
